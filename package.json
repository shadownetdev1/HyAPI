{
  "name": "hydrusapi",
  "version": "80.635.1-alpha",
  "description": "A JavaScript based implementation of the Hydrus client API with documentation, tests, and type definitions.",
  "main": "hydrusapi.js",
  "scripts": {
    "test": "cp scripts/pre-commit .git/hooks/pre-commit && vitest run --coverage --coverage.reporter=text --coverage.reportsDirectory=/tmp/hydrusapi_coverage",
    "test:watch": "cp scripts/pre-commit .git/hooks/pre-commit && vitest --coverage --coverage.reporter=text --coverage.reportsDirectory=/tmp/hydrusapi_coverage"
  },
  "keywords": [
    "hydrusnetwork",
    "hydrus",
    "hydrus-network"
  ],
  "files": [
    "types/",
    "CHANGELOG.MD",
    "hydrusapi.js",
    "license.txt",
    "README.MD"
  ],
  "license": "SEE LICENSE IN license.txt",
  "type": "commonjs",
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@types/node": "^24.2.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.32.0",
    "fs-jetpack": "^5.1.0",
    "globals": "^16.3.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
